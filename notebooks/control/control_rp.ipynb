        "fy_r = (L1_r + q2_r) * sp.sin(q1_r)\n",
        "f_rp = sp.Matrix([fx_r, fy_r])\n",
        "J_rp = f_rp.jacobian([q1_r, q2_r])\n",
        "sp.simplify(J_rp)\n"
      ]
